<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.aketzagonzalez.aeropuertosD.tablaPersonasController">
   <children>
      <TableView fx:id="tablaPersonas" prefWidth="700.0">
         <columns>
            <TableColumn fx:id="idTablaNombre" editable="false" prefWidth="340.7999938726425" text="Nombre" />
            <TableColumn fx:id="idTablaApellido" editable="false" minWidth="0.0" prefWidth="198.39999389648438" text="Apellidos" />
            <TableColumn fx:id="idTablaEdad" editable="false" minWidth="0.0" prefWidth="162.4000244140625" text="Edad" />
         </columns>
      </TableView>
      <Button fx:id="btnAniadir" mnemonicParsing="false" onAction="#aniadirPersona" text="Agregar Persona" />
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
